# Example terraform.tfvars for staging environment
# Copy this to terraform.tfvars and fill in the values
# IMPORTANT: Never commit terraform.tfvars to version control

project_id = "your-scaleway-project-id"

db_admin_password = "secure-admin-password-min-16-chars"
db_user_password  = "secure-user-password-min-16-chars"

redis_password = "secure-redis-password-min-16-chars"

paseto_access_key  = "base64-encoded-32-byte-key"
paseto_refresh_key = "base64-encoded-32-byte-key"

# admin_token = "secure-admin-token"

container_image_tag = "staging-stable"

domain_name = "ciel-social.eu"
enable_dns  = true

ssh_allowed_cidrs = ["1.2.3.4/32"]
